	<queryString>
		<![CDATA[SELECT 
   v.id,
    v.fecha,
    v.total,
    v.estado,
    v.numero,
    v.condicion,
    CONCAT(c.nombre) AS nombre_cliente,
    c.ci AS ci_cliente,

    CASE 
        WHEN v.condicion = 'contado' THEN 'X' 
        ELSE ' ' 
    END AS contado,
    CASE 
        WHEN v.condicion = 'credito' THEN 'X' 
        ELSE ' ' 
    END AS credito
   
FROM 
    public.ventas v
JOIN 
    public.clientes c ON v.id_cliente = c.id_cliente
WHERE 
    v.id =$P{detalleventa}]]>
	</queryString>
