	<queryString>
		<![CDATA[SELECT 
    dv.id,
    dv.cantidad,
    dv.precio,
    p.nombre AS producto,
    v.id AS id_venta
FROM 
    detalleventas dv
INNER JOIN 
    productos p ON dv.id_producto = p.id_producto
