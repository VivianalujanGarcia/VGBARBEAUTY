	<queryString>
		<![CDATA[SELECT
    cobros.idcobros AS idcobros,
    cobros.fecha AS fecha,
    cobros.estado AS estado,
    cobros.numero AS numero,
    cobros.idaperturasfk AS idaperturas,
    cobros.idclientesfk AS idclientes,
    clientes.nombre AS nombre,
    clientes.apellido AS apellido,
    clientes.ruc AS ruc,
    CONCAT(clientes.nombre, ' ', clientes.apellido) AS nombre_completo
FROM
    clientes
INNER JOIN cobros ON clientes.idclientes = cobros.idclientesfk
where idcobros = $P{cobros}]]>
	</queryString>
